{
  "name": "fyi-clone",
  "private": true,
  "devDependencies": {
    "lerna": "^3.19.0"
  },
  "version": "1.0.0",
  "description": "clone of fyi website with sanity and gatsby",
  "scripts": {
    "clean": "lerna clean",
    "deploy": "lerna run deploy",
    "empty-cache": "lerna run empty-cache",
    "init": "cd studio && sanity init && lerna run graphql-deploy",
    "format": "lerna run format",
    "graphql-deploy": "lerna run graphql-deploy",
    "lint": "lerna run lint",
    "now-deploy": "now && now alias",
    "postinstall": "lerna bootstrap",
    "sanity-deploy": "lerna run sanity-deploy",
    "start": "lerna run empty-cache && lerna run start --parallel",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kinla/fyi-clone.git"
  },
  "author": "Sharon Chien",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kinla/fyi-clone/issues"
  },
  "homepage": "https://github.com/Kinla/fyi-clone#readme"
}
